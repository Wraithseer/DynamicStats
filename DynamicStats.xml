<GuiXml>
    <Controls>
        <TopLevelControl name="DynamicStats_UI" mouseEnabled="true" movable="true" hidden="false">
            <Dimensions x="150" y="350" />
            <Anchor point="TOPLEFT" relativeTo="GuiRoot" relativePoint="CENTER" offsetX="0" offsetY="0"/>
            <Controls>
                <Backdrop name="$(parent)_Background" centerColor="000000" edgeColor="000000" alpha="0.7">
                    <Anchor point="TOPLEFT"  relativeTo="$(parent)" relativePoint="TOPLEFT"/>
                    <AnchorFill />
                    <Edge edgeSize="1" />
                </Backdrop>
                <Label name="$(parent)_DMG" font="esoui/common/fonts/Univers67.otf|22|shadow" text="test">
                    <Anchor point="TOP" offsetY="10" />
                </Label>
                <Label name="$(parent)_DMGDESC" font="esoui/common/fonts/Univers67.otf|10|shadow" text="DAMAGE/PENETRATION" color="82b8e0" alpha="0.8">
                    <Anchor point="TOP"  offsetY="2" />
                </Label>
                <Label name="$(parent)_CRIT" font="esoui/common/fonts/Univers67.otf|22|shadow" text="test">
                    <Anchor point="TOP" offsetY="41" />
                </Label>
                <Label name="$(parent)_CRITDESC" font="esoui/common/fonts/Univers67.otf|10|shadow" text="CRITICAL P/S/DMG" color="82b8e0" alpha="0.8">
                    <Anchor point="TOP" offsetY="33" />
                </Label>
                <Label name="$(parent)_RES" font="esoui/common/fonts/Univers67.otf|22|shadow" text="test">
                    <Anchor point="TOP" offsetY="72" />
                </Label>
                <Label name="$(parent)_RESDESC" font="esoui/common/fonts/Univers67.otf|10|shadow" text="RESISTANCE P/S" color="82b8e0" alpha="0.8">
                    <Anchor point="TOP" offsetY="64" />
                </Label>
       
                <Label name="$(parent)_SPEEDDESC" font="esoui/common/fonts/Univers67.otf|10|shadow" text="MOVESPEED" color="82b8e0" alpha="0.8">
                    <Anchor point="TOP" offsetY="95" />
                </Label>

                <Label name="$(parent)_SPEED" font="esoui/common/fonts/Univers67.otf|22|shadow" text="test">
                    <Anchor point="TOP" offsetY="103" />
                </Label>

                <Label name="$(parent)_DEBUG" font="esoui/common/fonts/Univers67.otf|22|shadow" text="test">
                    <Anchor point="TOP" offsetY="125" />
                </Label>
            </Controls>
            <OnMoveStop>
                DynamicStatsOnMoveStop()
            </OnMoveStop>
        </TopLevelControl>
    </Controls>
</GuiXml>